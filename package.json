{
  "name": "my-diary-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "build-start": "yarn build && yarn start",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "test": "jest",
    "playwright-vrt:test": "playwright test",
    "playwright-vrt:build": "docker compose build",
    "playwright-vrt:run": "docker compose run playwright yarn playwright-vrt:test",
    "playwright-vrt:update-screenshots": "docker compose run playwright yarn playwright-vrt:test --update-snapshots",
    "storybook": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "storycap:expected": "storycap ---outDir expected --serverCmd \"npx http-server storybook-static -p 6006\" http://localhost:6006",
    "storycap:actual": "storycap --outDir actual --serverCmd \"npx http-server storybook-static -p 6006\" http://localhost:6006",
    "storycap:diff": "npx reg-cli actual expected diff -R index.html && open index.html",
    "vrt:snapshot": "storycap --serverCmd \"npx http-server storybook-static -a localhost -p 6006\" http://localhost:6006",
    "vrt:run": "reg-suit run"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "core-js": "^3.25.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nuxt": "^3.7.1",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "vuetify": "^3.4.0-alpha.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/storybook": "^4.3.2",
    "@playwright/test": "^1.36.2",
    "@storybook/addon-viewport": "^7.3.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "5",
    "@types/jest": "^29.5.2",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.1.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^2.7.1",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.12.2",
    "reg-publish-s3-plugin": "^0.12.2",
    "reg-suit": "^0.12.2",
    "sass": "^1.66.1",
    "storycap": "^4.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-jest": "^3.0.4"
  }
}
